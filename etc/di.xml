        <plugin name="set-title-d"
                type="RobinMcK\PluginsAndObservers\Plugin\Challenge\SetTitleD" />
    </type>
    <type name="RobinMcK\PluginsAndObservers\Block\StaffPick">
        <arguments>
            <argument name="staffName" xsi:type="string">Robin</argument>
        </arguments>
    </type>
</config>
